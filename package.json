{
    "name": "vdp-demo",
    "version": "0.0.0",
    "scripts": {
        "start": "run-p develop local",
        "develop": "rollup -c rollup.config.js -w src/",
        "bundle": "rollup -c rollup.config.js",
        "local": "chokidar \"dist/*.js\" \"./develop.js\" \"./mock.json\" -c \"node develop.js\"",
        "zip": "zip -vr $(node -p \"Date.now()\").zip dist fonts package.json index.js",
        "discard": "rimraf *.zip",
        "pack": "run-s discard bundle zip"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "21.0.1",
        "@rollup/plugin-node-resolve": "13.1.1",
        "@rollup/plugin-typescript": "8.3.0",
        "@types/d3": "7.1.0",
        "@types/jsdom": "16.2.13",
        "@types/node": "16.10.1",
        "@types/react": "17.0.24",
        "chokidar-cli": "3.0.0",
        "d3": "7.2.1",
        "npm-run-all": "4.1.5",
        "rimraf": "3.0.2",
        "rollup": "2.61.1",
        "typescript": "*"
    },
    "dependencies": {
        "@react-pdf/renderer": "~2",
        "canvas": "2.8.0",
        "jsdom": "19.0.0",
        "react": "*",
        "tslib": "2.3.1"
    }
}
