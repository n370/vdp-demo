{
    "name": "vdp-demo",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "bundle": "rollup -c rollup.config.js",
        "zip": "zip -vr $(node -p \"Date.now()\").zip dist package.json index.js",
        "discard": "rimraf *.zip",
        "pack": "run-s discard bundle zip",
        "start:function": "node ./develop-function.js",
        "start:local": "node ./develop-local.js",
        "invoice": "run-p \"start:** invoice\"",
        "poster": "run-p \"start:** poster\"",
        "cv": "run-p \"start:** cv\"",
        "start": "npm run invoice"
    },
    "devDependencies": {
        "@google-cloud/functions-framework": "2.1.0",
        "@rollup/plugin-commonjs": "21.0.1",
        "@rollup/plugin-node-resolve": "13.1.1",
        "@rollup/plugin-typescript": "8.3.0",
        "@types/d3": "7.1.0",
        "@types/jsdom": "16.2.13",
        "@types/node": "16.10.1",
        "@types/react": "17.0.24",
        "chokidar": "3.5.3",
        "d3": "7.2.1",
        "kill-port": "1.6.1",
        "npm-run-all": "4.1.5",
        "rimraf": "3.0.2",
        "rollup": "2.61.1",
        "rollup-plugin-copy": "3.4.0",
        "typescript": "*"
    },
    "dependencies": {
        "@react-pdf/renderer": "3.1.11",
        "@types/uuid": "9.0.1",
        "axios": "1.4.0",
        "canvas": "2.11.2",
        "date-fns": "2.30.0",
        "jsdom": "22.1.0",
        "qrcode": "1.5.3",
        "react": "*",
        "tslib": "2.5.2",
        "uuid": "9.0.0"
    }
}
